{
  "name": "wikibase-cli",
  "version": "12.1.0",
  "description": "The command-line interface to Wikibase",
  "main": "index.js",
  "bin": {
    "wb": "./bin/wb",
    "wd": "./bin/wd"
  },
  "scripts": {
    "docker:publish": "./scripts/docker_publish",
    "lint": "standard bin/wb* lib/* lib/*/* test/* test/lib/*",
    "lint-staged": "./scripts/lint_staged",
    "prepublishOnly": "npm run lint && npm test",
    "postpublish": "npm run docker:publish && git push --tags",
    "test": "mocha --exit",
    "update-toc": "./scripts/update_toc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikidata-cli.git"
  },
  "files": [
    "bin",
    "lib",
    "metadata"
  ],
  "keywords": [
    "wikidata",
    "wikibase",
    "wb",
    "wd",
    "cli"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikidata-cli/issues"
  },
  "homepage": "https://github.com/maxlath/wikidata-cli#readme",
  "dependencies": {
    "@wikibasejs/commander": "^2.15.3",
    "chalk": "^2.4.2",
    "copy-paste": "^1.3.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.6.0",
    "open": "^7.0.4",
    "read": "^1.0.7",
    "split": "^1.0.1",
    "through": "^2.3.8",
    "wikibase-edit": "^4.4.1",
    "wikibase-sdk": "^7.6.0",
    "wikidata-lang": "^2.0.11"
  },
  "devDependencies": {
    "git-hooks": "^1.1.10",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "xit",
      "describe"
    ]
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
